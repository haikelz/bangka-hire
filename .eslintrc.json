{
  "extends": [
    "next",
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "@next/next/no-html-link-for-pages": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/no-explicit-any": "off"
  },
  "parserOptions": {
    "babelOptions": {
      "presets": [
        "next/babel"
      ]
    }
  },
  "plugins": [
    "jsx-a11y",
    "react",
    "@typescript-eslint"
  ]
}